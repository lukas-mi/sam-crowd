      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "They constitute a leguminous elite who can lead the way and drag others along"
          },
          {
            "type": "TextPositionSelector",
            "start": 4569,
            "end": 4646
          }
        ]
