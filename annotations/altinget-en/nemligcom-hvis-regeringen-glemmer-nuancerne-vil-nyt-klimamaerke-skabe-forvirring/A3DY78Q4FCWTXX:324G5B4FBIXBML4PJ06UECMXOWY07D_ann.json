      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "A system is needed which can support consumers in moving their choices in a more climate-friendly direction on a nuanced basis"
          },
          {
            "type": "TextPositionSelector",
            "start": 2089,
            "end": 2215
          }
