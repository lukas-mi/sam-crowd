      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "The brand and its information on the products must be able to create inspiration and knowledge during conversations, cooking and dinners"
