    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "A \"positive scheme\" risks creating uncertainty and confusion among consumers"
          },
          {
            "type": "TextPositionSelector",
            "start": 2372,
            "end": 2448
          }
