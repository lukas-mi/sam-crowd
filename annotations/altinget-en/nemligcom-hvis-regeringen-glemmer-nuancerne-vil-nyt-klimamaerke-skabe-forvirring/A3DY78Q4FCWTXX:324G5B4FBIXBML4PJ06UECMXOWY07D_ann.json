      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "This scheme can be compared to the EU's very successful energy label on electrical products"
          },
          {
            "type": "TextPositionSelector",
            "start": 3274,
            "end": 3365
          }
        ]
