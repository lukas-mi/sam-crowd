      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "It is important that the climate label is able to build a long-term understanding among consumers"
          },
          {
            "type": "TextPositionSelector",
            "start": 1990,
            "end": 2087
          }
        ]
      },
      "id": "#0a18f98a-9d33-4922-801a-08f6918e047c"
