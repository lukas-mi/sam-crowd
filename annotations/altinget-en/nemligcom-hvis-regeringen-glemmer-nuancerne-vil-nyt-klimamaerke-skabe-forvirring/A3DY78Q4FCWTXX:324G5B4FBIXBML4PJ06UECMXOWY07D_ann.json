      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "A graduated color scale across categories is not a good idea"
          },
          {
            "type": "TextPositionSelector",
