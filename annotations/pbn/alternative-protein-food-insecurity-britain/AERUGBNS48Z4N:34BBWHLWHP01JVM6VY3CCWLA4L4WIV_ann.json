      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "Premise"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "alternative proteins produce an average of 88 percent lessgreenhouse gas emissions than animal proteins, helping us on our way to Net Zero by 2050"
          },
          {
            "type": "TextPositionSelector",
            "start": 4183,
            "end": 4329
          }
        ]
      },
      "id": "#0e3c6b8a-706b-4466-a2ec-2206ab0df5ea"
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "as well as giving us food security, this could reduce the cost of living through lower food prices and create tens of thousands of jobs"
          },
          {
            "type": "TextPositionSelector",
            "start": 4362,
            "end": 4497
          }
        ]
