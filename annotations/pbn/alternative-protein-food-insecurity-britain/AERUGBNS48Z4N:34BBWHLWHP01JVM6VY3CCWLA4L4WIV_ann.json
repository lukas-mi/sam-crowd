    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "Premise"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "Instead of using vast amounts of land to produce food for animals, we can produce the same amount of food on 93 percent less land, using 95 percent less water and with far less Russian fertilizer \u2013 safeguarding British farming through more efficient use of arable land"
          },
          {
            "type": "TextPositionSelector",
            "start": 3891,
            "end": 4159
          }
        ]
