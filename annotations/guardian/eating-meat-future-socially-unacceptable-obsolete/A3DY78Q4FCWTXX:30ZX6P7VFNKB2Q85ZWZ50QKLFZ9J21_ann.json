      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "Carnivores are going the way of cigarette smokers"
          },
          {
            "type": "TextPositionSelector",
            "start": 163,
            "end": 212
          }
        ]
      },
      "id": "#2258de30-2b2f-490f-b826-0d31b34e3d49"
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
