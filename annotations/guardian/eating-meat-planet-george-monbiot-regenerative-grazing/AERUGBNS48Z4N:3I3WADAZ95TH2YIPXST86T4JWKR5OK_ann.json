    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "Premise"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "Before long, most of our food will come neither from animals nor plants, but from unicellular life,\u201d he wrote in this paper in 2020"
          },
          {
            "type": "TextPositionSelector",
            "start": 1847,
            "end": 1978
          }
