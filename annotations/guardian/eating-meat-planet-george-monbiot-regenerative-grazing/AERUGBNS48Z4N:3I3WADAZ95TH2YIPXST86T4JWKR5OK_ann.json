    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "type": "Annotation",
      "body": [
        {
          "type": "TextualBody",
          "purpose": "tagging",
          "value": "ClaimFor"
        }
      ],
      "target": {
        "selector": [
          {
            "type": "TextQuoteSelector",
            "exact": "I am much more for small-scale, community-driven farming"
          },
          {
            "type": "TextPositionSelector",
            "start": 2031,
            "end": 2087
          }
        ]
      },
      "id": "#223ec51e-7828-4287-b8b5-4b17349b14d5"
